  "name": "@bsv/sdk",
  "version": "1.4.0",
  "type": "module",
  "description": "BSV Blockchain Software Development Kit",
  "main": "dist/cjs/mod.js",
  "module": "dist/esm/mod.js",
  "types": "dist/types/mod.d.ts",
